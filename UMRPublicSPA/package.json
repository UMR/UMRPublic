{
    "name": "umr-public-spa",
    "version": "1.0.0",
    "license": "ISC",
    "scripts": {
        "rimraf": "rimraf",
        "clean:dist": "npm run rimraf -- dist .awcache",
        "clean:compile": "npm run rimraf -- \"app/compiled\" ",
        "ngc": "./node_modules/.bin/ngc -p tsconfig.aot.json --locale=en-US && npm run rimraf -- output",
        "start:prod:aot": "npm run clean:dist && npm run clean:compile && npm run ngc && webpack-dev-server --progress --config webpack.prod.aot.config.js --port 14247 --open",
        "build:prod:aot": "npm run clean:dist && npm run clean:compile && npm run ngc && webpack --progress --config webpack.prod.aot.config.js",
        "start:prod:jit": "npm run clean:dist && npm run clean:compile && webpack-dev-server --progress --config webpack.prod.jit.config.js --port 14247 --open",
        "build:prod:jit": "npm run clean:dist && npm run clean:compile && webpack --progress --config webpack.prod.jit.config.js",
        "start:dev": "npm run clean:dist && npm run clean:compile && webpack-dev-server --progress --watch --output-pathinfo --config webpack.dev.config.js",
        "build:dev": "npm run clean:dist && npm run clean:compile && webpack --progress --config webpack.dev.config.js"
    },
    "devDependencies": {
        "@types/core-js": "0.9.35",
        "@types/node": "6.0.54",
        "@types/fullcalendar": "2.7.36",
        "angular2-router-loader": "0.3.4",
        "angular2-template-loader": "0.6.0",
        "awesome-typescript-loader": "3.0.0-beta.17",
        "compression-webpack-plugin": "0.3.2",
        "copy-webpack-plugin": "4.0.1",
        "html-webpack-plugin": "2.24.1",
        "jasmine-core": "2.5.2",
        "karma": "1.3.0",
        "karma-chrome-launcher": "2.0.0",
        "karma-cli": "1.0.1",
        "karma-htmlfile-reporter": "0.3.4",
        "karma-jasmine": "1.1.0",
        "open-browser-webpack-plugin": "0.0.3",
        "protractor": "4.0.14",
        "raw-loader": "0.5.1",
        "rimraf": "2.5.4",
        "source-map-loader": "0.1.5",
        "typescript": "2.1.4",
        "typings": "2.1.0",
        "webpack": "2.1.0-beta.25",
        "webpack-dev-server": "2.1.0-beta.9"
    },
    "dependencies": {
        "@angular/common": "2.4.1",
        "@angular/compiler": "2.4.1",
        "@angular/compiler-cli": "2.4.1",
        "@angular/core": "2.4.1",
        "@angular/forms": "2.4.1",
        "@angular/http": "2.4.1",
        "@angular/platform-browser": "2.4.1",
        "@angular/platform-browser-dynamic": "2.4.1",
        "@angular/platform-server": "2.4.1",
        "@angular/router": "3.4.1",
        "@angular/upgrade": "2.4.1",
        "@angular/tsc-wrapped": "0.5.0",
        "angular-in-memory-web-api": "0.2.2",
        "angular2-cookie": "1.2.6",
        "bootstrap": "3.3.7",
        "core-js": "2.4.1",
        "es6-shim": "0.35.2",
        "font-awesome": "4.7.0",
        "reflect-metadata": "0.1.9",
        "rxjs": "5.0.2",
        "zone.js": "0.7.4"
    }
}
