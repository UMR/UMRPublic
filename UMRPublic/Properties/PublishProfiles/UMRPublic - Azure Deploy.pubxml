<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <WebPublishMethod>MSDeploy</WebPublishMethod>
    <LastUsedBuildConfiguration>Release</LastUsedBuildConfiguration>
    <LastUsedPlatform>Any CPU</LastUsedPlatform>
    <SiteUrlToLaunchAfterPublish>http://umrpublic.azurewebsites.net</SiteUrlToLaunchAfterPublish>
    <LaunchSiteAfterPublish>True</LaunchSiteAfterPublish>
    <ExcludeApp_Data>False</ExcludeApp_Data>
    <MSDeployServiceURL>umrpublic.scm.azurewebsites.net:443</MSDeployServiceURL>
    <DeployIisAppPath>UMRPublic</DeployIisAppPath>
    <RemoteSitePhysicalPath />
    <SkipExtraFilesOnServer>True</SkipExtraFilesOnServer>
    <MSDeployPublishMethod>WMSVC</MSDeployPublishMethod>
    <EnableMSDeployBackup>True</EnableMSDeployBackup>
    <UserName>$UMRPublic</UserName>
    <_SavePWD>True</_SavePWD>
    <PublishDatabaseSettings>
      <Objects xmlns="">
        <ObjectGroup Name="UMRJobsEntities" Order="1" Enabled="False">
          <Destination Path="Data Source=SUNDAY;Initial Catalog=UMR_DIAGNOSIS_000;User ID=sa" />
          <Object Type="DbCodeFirst">
            <Source Path="DBContext" DbContext="UMRPublic.EntityFramework.UMRJobsEntities, UMRPublic" Origin="Configuration" />
          </Object>
        </ObjectGroup>
      </Objects>
    </PublishDatabaseSettings>
  </PropertyGroup>
  <ItemGroup>
    <MSDeployParameterValue Include="$(DeployParameterPrefix)UMRJobsEntities-Web.config Connection String">
      <ParameterValue>metadata=res://*/EntityFramework.UMRJobsModel.csdl|res://*/EntityFramework.UMRJobsModel.ssdl|res://*/EntityFramework.UMRJobsModel.msl;provider=System.Data.SqlClient;provider connection string="Data Source=SUNDAY;Initial Catalog=UMR_DIAGNOSIS_000;User ID=sa"</ParameterValue>
      <UpdateDestWebConfig>False</UpdateDestWebConfig>
    </MSDeployParameterValue>
  </ItemGroup>
</Project>